{
  "ignore": [
    "**/*.test.ts",
    "**/*.spec.ts",
    ".git",
    "node_modules/**/node_modules",
    "public/**/*.json",
    "public/**/*.csv",
    "public"

  ],
  "watch": [
    "./"
  ],
  "exec": "node --inspect index.js",
  "ext": "js,json",
  "restartable": "rs",
  "verbose": true,
  "env": {
    "NODE_ENV": "development",
    "NODE_DOCKER_ENV": "0"
  }
}